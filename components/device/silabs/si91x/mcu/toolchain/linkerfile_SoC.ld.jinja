   {%- else %}
   __main_flash_end__ = 0x{{ '%0x' % app_flash_start }} + 0x{{ '%0x' % app_flash_size }};
  {% endif %}
	   /* This is where we handle flash storage blocks. We use dummy sections for finding the configured
